// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./deb.db"
}

model Customer {
  id String @default(cuid()) @id
  name String
  second_name String
  plan Plan[]
  debts Int
  address String
  region Region[]
}

model Region {
  id String @default(cuid()) @id
  name String
  price_for_region Int
  customer Customer[]
}

model Plan {
  id String @default(cuid()) @id
  name String @unique
  default_price Int
  users Customer[]
}